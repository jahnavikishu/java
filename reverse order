package java2;

import java.util.Scanner;

public class Pat8 {
	public static void main(String[] args) {
		Scanner sc =new Scanner(System.in);
		System.out.println("enter size");
		int n =sc.nextInt();
		int count=n*n;
		for(int i=1;i<=n;i++) {
			
			for(int j=1;j<=n;j++){
	
			System.out.print(count);
			System.out.print("\t");
			count--;
		}
			System.out.println();
		}

}
}
